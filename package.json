{
  "name": "react-parcel-starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "npm run generate-imported-components && parcel src/client/index.html",
    "dev:server": "nodemon -e js,jsx,html --ignore dist --ignore src/client/imports.js --exec 'npm run build && npm run start'",
    "build": "rimraf dist && npm run generate-imported-components && npm run create-bundles",
    "create-bundles": "concurrently \"npm run create-bundle:client\" \"npm run create-bundle:server\"",
    "create-bundle:client": "cross-env BABEL_ENV=client parcel build src/client/index.html -d dist/client --public-url /dist/client --no-source-maps",
    "create-bundle:server": "cross-env BABEL_ENV=server parcel build src/server/index.js -d dist/server --public-url /dist --target=node --no-source-maps",
    "generate-imported-components": "imported-components app src/client/imports.js",
    "start": "node dist/server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "react-imported-component": "^6.2.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "llog": "^0.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "postcss": "^7.0.26",
    "postcss-modules": "^1.5.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.18",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.0",
    "styled-components": "^5.0.0",
    "through": "^2.3.8"
  }
}
