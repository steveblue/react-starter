{
  "name": "react-parcel-starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development npm run generate-imported-components && parcel src/client/index.html --hmr-port 1235 --port 3200",
    "dev:ssr": "NODE_ENV=development nodemon -e js,jsx,ts,tsx,html,css --ignore dist --ignore src/client/imports.js --exec \"npm run ssr && npm run serve\"",
    "ssr": "rimraf dist && npm run generate-imported-components && npm run build:ssr",
    "prod": "NODE_ENV=production rimraf dist && npm run generate-imported-components && npm run build:prod",
    "build:ssr": "concurrently \"npm run ssr:client\" \"npm run ssr:server\"",
    "build:prod": "concurrently \"npm run prod:client\" \"npm run prod:server\"",
    "ssr:client": "cross-env BABEL_ENV=client parcel build src/client/index.html -d dist/client --public-url /dist/client",
    "ssr:server": "cross-env BABEL_ENV=server parcel build src/server/index.ts -d dist/server --public-url /dist --target=node",
    "prod:client": "cross-env BABEL_ENV=client parcel build src/client/index.html -d dist/client --public-url /dist/client --no-source-maps",
    "prod:server": "cross-env BABEL_ENV=server parcel build src/server/index.ts -d dist/server --public-url /dist --target=node --no-source-maps",
    "generate-imported-components": "imported-components app src/client/imports.js",
    "serve": "node dist/server",
    "test": "jest",
    "pretest": "npm run lint",
    "lint": "tslint src/**/*.ts src/**/*.tsx --fix",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "pretty:check": "prettier \"src/**/*.{js,jsx,ts,tsx,html,css}\" --check",
    "pretty": "prettier \"src/**/*.{js,jsx,ts,tsx,html,css}\" --write"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "react-imported-component": "^6.2.1",
    "react-intl": "^3.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-actions": "^5.3.6",
    "@storybook/addon-links": "^5.3.6",
    "@storybook/addons": "^5.3.6",
    "@storybook/react": "^5.3.6",
    "@types/cheerio": "^0.22.15",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-transition-group": "^4.2.3",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-transform-css": "^2.0.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "postcss": "^7.0.26",
    "postcss-modules": "^1.5.0",
    "prettier": "1.19.1",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.18",
    "react-is": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "react-transition-group": "^4.3.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.5"
  }
}
