{
  "name": "react-parcel-starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "rimraf dist && npm run generate-imported-components && npm run build:dev",
    "prod": "rimraf dist && npm run generate-imported-components && npm run build:prod",
    "build:dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build:prod": "concurrently \"npm run prod:client\" \"npm run prod:server\"",
    "dev:client": "cross-env BABEL_ENV=client parcel build src/client/index.html -d dist/client --public-url /dist/client",
    "dev:server": "cross-env BABEL_ENV=server parcel build src/server/index.js -d dist/server --public-url /dist --target=node",
    "prod:client": "cross-env BABEL_ENV=client parcel build src/client/index.html -d dist/client --public-url /dist/client --no-source-maps",
    "prod:server": "cross-env BABEL_ENV=server parcel build src/server/index.js -d dist/server --public-url /dist --target=node --no-source-maps",
    "generate-imported-components": "imported-components app src/client/imports.js",
    "serve": "nodemon -e js,jsx,html --ignore dist --ignore src/client/imports.js --exec 'npm run build:dev && npm run start'",
    "start": "node dist/server",
    "test": "jest",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "react-imported-component": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-actions": "^5.3.6",
    "@storybook/addon-links": "^5.3.6",
    "@storybook/addons": "^5.3.6",
    "@storybook/react": "^5.3.6",
    "@types/react": "^16.9.17",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-transform-css": "^2.0.0",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "postcss": "^7.0.26",
    "postcss-modules": "^1.5.0",
    "postcss-nested": "^4.2.1",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.18",
    "react-is": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "styled-components": "^5.0.0",
    "through": "^2.3.8"
  }
}
